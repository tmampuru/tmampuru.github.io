{% extends 'base.html.twig' %}

{% block stylesheets %}
	{#{{ encore_entry_link_tags('app') }}#}
	<link rel="stylesheet" href="{{ asset('css/main-style.css') }}"/>
	<link rel="stylesheet" href="{{ asset('css/dash-style.css') }}"/>
{% endblock %}

{% block body %}
	<div class="grid-container">
		{{ include('inc/navbar.html.twig') }}
		<main class="main">
			<div class="member-container">
				<div class="main-cards">
					<div class="card-light">
						<ul class="responsive-table">
							<li class="table-header">
								<div class="header-item">Members</div>
								<div class="header-item">Call Day Metric</div>
								<div class="header-item">Ticket Day Metric</div>
								<div class="header-item">Chat Day Metric</div>
							</li>
							<li class="table-row">
								<div class="">232</div>
								<div class="">8</div>
								<div class="">8</div>
								<div class="">8</div>
							</li>
						</ul>
					</div>
				</div>
				<div class="container">
					<hr>
					<a id="next">next</a>
					<a id="prev">prev</a>
					<div class="card-light">
					<div class="header_custome">
					<span>
						<svg xmlns="http://www.w3.org/2000/svg" width="30px" viewBox="0 0 388.227 388.227"  fill="#fff" xmlns:v="https://vecta.io/nano"><path d="M194.113 190.505c36.691 0 66.438-36.129 66.438-80.695s-9.768-80.696-66.438-80.696-66.438 36.129-66.438 80.696 29.747 80.695 66.438 80.695zm125.342 119.954c-1.229-77.637-11.369-99.759-88.959-113.763 0 0-10.924 13.917-36.381 13.917s-36.379-13.917-36.379-13.917c-76.744 13.85-87.502 35.645-88.916 111.24l-.189 5.78.01 8.157s18.473 37.239 125.475 37.239 125.477-37.239 125.477-37.239l.004-6.033c-.021.443-.064-.416-.142-5.381zm-33.142-134.362c29.801 0 53.959-29.343 53.959-65.539s-7.932-65.54-53.959-65.54c-7.742 0-14.404.833-20.135 2.388 10.631 19.598 12.088 43.402 12.088 62.403 0 21.514-5.832 42.054-16.572 59.061 7.382 4.61 15.747 7.227 24.619 7.227zm101.798 97.424c-1-63.055-9.234-81.022-72.252-92.396 0 0-8.871 11.304-29.547 11.304a54.9 54.9 0 0 1-2.5-.063c13.137 5.923 25.088 14.17 33.889 26.238 15.215 20.863 18.713 48.889 19.435 90.062 42.397-8.378 51.086-25.873 51.086-25.873l.004-4.913c-.017.376-.052-.298-.115-4.359zm-286.199-97.424c8.873 0 17.236-2.617 24.621-7.226-10.74-17.007-16.572-37.547-16.572-59.061 0-19.002 1.457-42.806 12.086-62.403-5.73-1.555-12.391-2.388-20.135-2.388-46.027 0-53.957 29.343-53.957 65.54s24.158 65.538 53.957 65.538zm2.5 16.268l-2.5.063c-20.676 0-29.547-11.304-29.547-11.304C9.349 192.498 1.113 210.464.115 273.52L0 277.878l.004 4.913s8.69 17.495 51.084 25.873c.725-41.172 4.221-69.198 19.438-90.062 8.8-12.066 20.749-20.314 33.886-26.237z"/></svg>
					</span>
						<h2>Roster</h2>
					</div>
					  {% if agents %}
						<ul class="responsive-table">
							<li class="table-header">
								<div class="header-item">Extention</div>
								<div class="header-item">Agent Name</div>
								<div class="header-item">Category</div>
								<div class="header-item">Vetting Status</div>
							</li>
							{% for agents in agents %}
							<li class="table-row">
								<div class="">{{ agents.extension}}</div>
								<div class="">{{ agents.agentName}}</div>
								<div class="">{{ agents.serviceArea}}</div>
								<div class="call status ">Call</div>
							</li>
							 {% endfor %}
						</ul>
						{% else %}
							<p>No agents to display</p>
						{% endif %}
					</div>
				</div>
			</div>
		</main>
		{{ include('inc/footer.html.twig') }}
	</div>
{% endblock %}
